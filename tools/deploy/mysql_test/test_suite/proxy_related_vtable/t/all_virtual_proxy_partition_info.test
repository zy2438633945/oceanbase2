eval select part_name, sub_part_position from oceanbase.__all_virtual_proxy_sub_partition where tenant_name = 'mysql' and table_id = $table_id;
--enable_query_log

connection default;
--disable_warnings
drop table if exists proxy_list_hash;
--enable_warnings

disconnect obsys;
