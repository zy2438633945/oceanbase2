--error 1367
SELECT ST_ASTEXT(MULTIPOINT('NULL', 'NULL'));
--error 1210
SELECT ST_ASTEXT(MULTIPOINT(LINESTRING(POINT(0,0),POINT(1,0))));

SELECT ST_ASTEXT(MULTIPOINT(POINT(0,0)));
SELECT ST_ASTEXT(MULTIPOINT(POINT(0,0),POINT(1,1)));
}

